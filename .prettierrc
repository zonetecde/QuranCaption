{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"useTabs": true,
	"printWidth": 100,
	"proseWrap": "always",
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "none",
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
